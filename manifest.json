{
    "manifest_version": 3,
    "name": "Open Comment Section new test",
    "version": "1.0",
    "description": "An extension to open a comment section on the current tab.",
    "minimum_chrome_version": "116",
    "background": {
      "service_worker": "service-worker.js"
    },
    "permissions": ["identity","sidePanel", "activeTab", "scripting","tabs","storage"],
    "oauth2": {
      "client_id": "622379932733-unuj4jk9bcdaemdv5q77fnp29lv5idd0.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/userinfo.profile", "https://www.googleapis.com/auth/userinfo.email"]
    },
    "icons": {
      "16": "images/2.png",
      "48": "images/2.png",
      "128": "images/2.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/2.png",
        "48": "images/2.png",
        "128": "images/2.png"
      }
    },
    "side_panel": {
      "default_path": "sidepanel-tab.html"
    }
  }
  